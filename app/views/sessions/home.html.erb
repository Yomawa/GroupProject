<% @schools.each do |school| %>
  <div class="panel panel-default text-center image ">
    <div class="panel-body ">
      <%= link_to school do %>
        <img src="<%= school.logo %>" class="logoSchool">
      <% end %>
    </div>
    <ul class="list-group school_name">
      <li class="list-group-item">
        <%= school.name %>
      </li>
    </ul>
  </div>
<% end %>

